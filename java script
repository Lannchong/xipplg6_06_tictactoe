SISWA (NISN PK, nama, tgl_lahir, jk, alamat)
   | 1
   | N
KELAS (kode_kelas PK, tingkat, wali_kelas FK->GURU)

GURU (NIP PK, nama, telp)
   | 1
   | N
JADWAL (kode_jadwal PK, hari, jam, kode_mapel FK, kode_kelas FK, guru FK)

MATA_PELAJARAN (kode_mapel PK, nama_mapel, durasi)

NILAI (id_nilai PK, NISN FK, kode_mapel FK, nilai)
